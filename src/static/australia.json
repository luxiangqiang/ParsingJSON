        "Leave Policy": [
            {
                "title": "Maternity Leave",
                "data": [
                    {
                        "type": "paragraph",
                        "data": "Pregnant employees who have worked for 12 months are entitled to 18 weeks of paid leave. The remaining days can be divided into periods of a minimum of 1 week. The employee will receive national minimum wage during this period and Social Security will be responsible for this pay."
                    },
                    {
                        "type": "paragraph",
                        "data": "The employee can extend leave additional 24 months unpaid via parental leave."
                    }
                ]
            },
            {
                "title": "Paternity Leave",
                "data": [
                    {
                        "type": "text",
                        "data": "Employees who have worked for 12 months are entitled to 2 weeks of paid paternity leave. The employee will receive national minimum wage during this period and Social Security will be responsible for this pay."
                    }
                ]
            },
            {
                "title": "Parental Leave",
                "data": [
                    {
                        "type": "text",
                        "data": "In addition to the paid maternity leave, employees are entitled to 12 months of parental leave. Parental leave can be extended an additional 12 months. Both parents can decide how to distribute the time off. Either parent can take parental leave. This leave is unpaid."
                    }
                ]
            },
            {
                "title": "Sick Leave",
                "data": [
                    {
                        "type": "text",
                        "data": "Employees are entitled to paid sick leave for up to 10 days. Employees receive their normal wage during this period which will be paid by the employer."
                    }
                ]
            }
        ],
        "Termination": [
            {
                "title": "Termination Requirements",
                "data": [
                    {
                        "type": "paragraph",
                        "data": "Terminations must respect complex rules and the rules of an employee’s employment country. The off-boarding is always handled by the Employer with the primary stakeholders. It may include ad-hoc fees as well as required or recommended steps on specific termination cases."
                    },
                    {
                        "type": "paragraph",
                        "data": "Terminations in Australia can be complex. There is no at-will termination in Australia for employers outside the probation period and termination must be done for just cause."
                    },
                    {
                        "type": "paragraph",
                        "data": "Compliant terminations include:"
                    },
                    {
                        "type": "nested_list",
                        "data": [
                            {
                                "text": "Voluntarily by the employee"
                            },
                            {
                                "text": "By mutual agreement"
                            },
                            {
                                "text": "Unilaterally by the employer based on:",
                                "children": [
                                    {
                                        "text": "Probation period"
                                    },
                                    {
                                        "text": "Objective grounds"
                                    },
                                    {
                                        "text": "Disciplinary dismissal"
                                    },
                                    {
                                        "text": "Performance due to unsuitability for the job"
                                    }
                                ]
                            },
                            {
                                "text": "By the expiration of the contract"
                            }
                        ]
                    }
                ]
            },
            {
                "title": "Notice Period",
                "data": [
                    {
                        "type": "paragraph",
                        "data": "The minimum notice period is 1 week and will be increased according to the length of the employment."
                    },
                    {
                        "type": "nested_list",
                        "data": [
                            {
                                "text": "Less than 1 year of employment - 1 week’s notice"
                            },
                            {
                                "text": "1 - 3 years of employment - 2 weeks’ notice"
                            },
                            {
                                "text": "3 - 5 years of employment - 3 weeks’ notice"
                            },
                            {
                                "text": "5 years of employment - 4 weeks’ notice"
                            }
                        ]
                    },
                    {
                        "type": "paragraph",
                        "data": "In case the employee is older than 45 years old and has worked for the same employer for 2 years, they will be entitled to an additional week of notice."
                    }
                ]
            },
            {
                "title": "Severance for Employees",
                "data": [
                    {
                        "type": "paragraph",
                        "data": "Severance pay will depend on the number of years an employee has worked at the company."
                    },
                    {
                        "type": "nested_list",
                        "data": [
                            {
                                "text": "Less than 1 year at the company receives no severance pay."
                            },
                            {
                                "text": "1-2 years = 4 weeks’ severance pay."
                            },
                            {
                                "text": "2-3 years = 6 weeks’ severance pay."
                            },
                            {
                                "text": "3-4 years = 7 weeks’ severance pay."
                            },
                            {
                                "text": "4-5 years = 8 weeks’ severance pay."
                            },
                            {
                                "text": "5-6 years = 10 weeks’ severance pay."
                            },
                            {
                                "text": "6-7 years = 11 weeks’ severance pay."
                            },
                            {
                                "text": "7-8 years = 13 weeks’ severance pay."
                            },
                            {
                                "text": "8-9 years = 14 weeks’ severance pay."
                            },
                            {
                                "text": "More than 10 years = 16 weeks’ severance pay."
                            }
                        ]
                    }
                ]
            }
